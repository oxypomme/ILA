<UserControl x:Class="ilaGUI.classes.Assign"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ilaGUI.classes"
             mc:Ignorable="d"
             d:DesignHeight="16" d:DesignWidth="200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid Name="leftGrid" VerticalAlignment="Top" Height="16" Grid.Column="0" />
        <TextBlock Text="=" Margin="5,0,5,0" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White" />
        <Grid Name="rightGrid" VerticalAlignment="Top" Height="16" Grid.Column="2" />
    </Grid>
</UserControl>
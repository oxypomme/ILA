<UserControl x:Class="ilaGUI.TreeElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ilaGUI"
             mc:Ignorable="d"
             d:DesignHeight="20" d:DesignWidth="200">
    <Grid>
        <Image x:Name="Icon" HorizontalAlignment="Left" Stretch="None" VerticalAlignment="Top" />
        <Label Name="Title" Content="Label" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Margin="20,-3,0,0" />
    </Grid>
</UserControl>
<Window x:Class="ilaGUI.NewFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ilaGUI"
        mc:Ignorable="d"
        Title="Nouvel Algo" Height="83" Width="204" ResizeMode="NoResize" ShowInTaskbar="False" SizeToContent="WidthAndHeight" Background="#FF222222" Closing="Window_Closing">
    <Grid Height="68">
        <Label Content="Nom de l'algo :" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="11,12,0,0" Foreground="White" Background="{x:Null}" />
        <TextBox Name="algonameTB" Margin="106,15,0,34" Text="Algo1" TextWrapping="Wrap" HorizontalAlignment="Left" Width="88" />
        <Button Name="cancelBtn" Content="Annuler" HorizontalAlignment="Left" Margin="20,0,0,15" Width="64" Height="16" VerticalAlignment="Bottom" Click="cancelBtn_Click" />
        <Button Name="newBtn" Content="Créer" Margin="0,0,20,15" HorizontalAlignment="Right" Width="64" Height="16" VerticalAlignment="Bottom" />
    </Grid>
</Window>
<UserControl x:Class="ilaGUI.Editor.FunctionCall"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ilaGUI.Editor"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <TextBlock x:Name="fctName" FontFamily="{DynamicResource firacode}" Foreground="{StaticResource moduleColor}" />
        <TextBlock FontFamily="{DynamicResource firacode}" Foreground="{StaticResource symbolColor}" Text="(" />
        <StackPanel Orientation="Horizontal" Name="parameters" />
        <TextBlock FontFamily="{DynamicResource firacode}" Foreground="{StaticResource symbolColor}" Text=")" />
    </StackPanel>
</UserControl>
<UserControl x:Class="ilaGUI.Editor.TableCall"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ilaGUI.Editor"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <ContentControl x:Name="variable" />
        <TextBlock x:Name="linkToFont" Text="[" FontFamily="{DynamicResource firacode}" Foreground="{StaticResource symbolColor}" />
        <StackPanel Orientation="Horizontal" Name="parameters" />
        <TextBlock Text="]" FontFamily="{DynamicResource firacode}" Foreground="{StaticResource symbolColor}" />
    </StackPanel>
</UserControl>